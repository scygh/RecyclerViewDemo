<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.scy.recyclerviewdemo">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".waterfall.WaterFallActivity"></activity>
        <activity android:name=".timeline.TimelineActivity" />
        <activity android:name=".vertical.VerticalActivity" />
        <activity android:name=".swipe.SwipeActivity" />
        <activity android:name=".sticky.StickyActivity" />
        <activity android:name=".snaphelper.SnaphelperActivity" />
        <activity android:name=".slide.SlideActivity" />
        <activity android:name=".refresh.refreshActivity" />
        <activity android:name=".load.loadActivity" />
        <activity android:name=".link.LinkActivity" />
        <activity android:name=".horizontal.HorizontalActivity" />
        <activity android:name=".header.HeaderActivity" />
        <activity android:name=".group.GroupActivity" />
        <activity android:name=".gird.GirdActivity" />
        <activity android:name=".footer.FooterActivity" />
        <activity android:name=".ExpandCollapse.ExpandCollapseActivity" />
        <activity android:name=".drag.DragActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".click.RcvClickActivity" />
    </application>

</manifest>